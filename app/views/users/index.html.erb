<p id="notice"><%= notice %></p>
<div class="col-md-8">
<h1>ようこそ</h1>
<p>トップページです。</p>

<table border="1" cellspacing='5'>
  <thead>
    <tr>
      <th>投稿画像</th>
      <th>作成日時</th>
      <th>名前</th>
      <th>年齢</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= image_tag user.image.to_s, style: "width: 100px;" %></td>
        <td><%= user.created_at.strftime('%Y-%m-%d %H-%M-%S') %></td>
        <td><%= user.name %></td>
        <td><%= user.age %></td>
        <td><%= link_to 'Show', user, class: "square_btn", style: "margin: 5px;" %></td>
        <td><%= link_to 'Edit', edit_user_path(user), class: "btn btn-success", style: "margin: 5px;" %></td>
        <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-danger", style: "margin: 5px;" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= flash[:message] %>
<br>

